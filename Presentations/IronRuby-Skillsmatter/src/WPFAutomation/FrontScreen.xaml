<Window x:Class="WPFAutomation.FrontScreen"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="FrontScreen" Height="232" Width="514">
    <Grid>
        <Button Margin="70,88,0,83" Name="calculateButton" Click="saveButton_Click" HorizontalAlignment="Left" Width="89">Calculate</Button>
        <TextBox Height="27" Margin="0,22,230,0" Name="priceText" VerticalAlignment="Top" HorizontalAlignment="Right" Width="192" />
        <Label Height="28" HorizontalAlignment="Left" Margin="12,21,0,0" Name="priceLabel" VerticalAlignment="Top" Width="52">Price</Label>
        <TextBox Height="27" Margin="70,55,0,0" Name="percentageText" VerticalAlignment="Top" HorizontalAlignment="Left" Width="48" />
        <Label Height="28" HorizontalAlignment="Left" Margin="12,54,0,0" Name="percentageLabel" VerticalAlignment="Top" Width="52">%</Label>
        <Label Margin="0,0,230,29" Name="costLbl" Height="38" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="192"></Label>
        <Label Height="28" HorizontalAlignment="Left" Margin="12,0,0,39" Name="costLabel" VerticalAlignment="Bottom" Width="36">Cost</Label>
    </Grid>
</Window>
